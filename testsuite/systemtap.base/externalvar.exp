set test "externalvar"
set testpath "$srcdir/$subdir"
set testsrc  "$testpath/$test.c"
set testsrclib  "$testpath/${test}_lib.c"
set testexe  "[pwd]/[stap_host_testdir]/$test"
set testlibname  "$test"
set testlibdir "[pwd]"
set target_testlibdir "[stap_target_testdir]"
set testso  "$testlibdir/[stap_host_testdir]/lib${testlibname}.so"
set testflags "additional_flags=-g additional_flags=-O0"
set testlibflags "$testflags additional_flags=-fPIC additional_flags=-shared"
set maintestflags "$testflags additional_flags=-L$testlibdir/[stap_host_testdir] additional_flags=-l$testlibname additional_flags=-Wl,-rpath,$target_testlibdir"

# Only run on make installcheck and utrace present.
if {! [installtest_p]} { untested "$test"; return }
if {! [utrace_p]} { untested "$test"; return }
if {! [uprobes_p]} { untested "$test"; return }

# Compile our test program and library.
set res [target_compile $testsrclib $testso executable $testlibflags]
if { $res != "" } {
    verbose "target_compile for $testso failed: $res" 2
    fail "unable to compile $testsrclib"
    return
}
set res [target_compile $testsrc $testexe executable $maintestflags]
if { $res != "" } {
    verbose "target_compile failed: $res" 2
    fail "unable to compile $testsrc"
    return
}

set output {exevar_c = 42
exevar_i = 2
exevar_l = 21
exe_s->i = 1
exe_s->l = 2
exe_s->c = 3
exe_s->s1 = 0x0
exe_s == exe_s->s2
libvar = 42
lib_s->i = 1
lib_s->l = 2
lib_s->c = 3
lib_s == lib_s->s1
lib_s->s2 = 0x0}

set testexe_short [file tail $testexe]
if [is_remote target] {
  set testso_short [file tail $testso]
  remote_download target $testexe "[stap_target_testdir]/$testexe_short"
  remote_download target $testso "[stap_target_testdir]/$testso_short"
}

# Got to run stap with both the exe and the libraries used as -d args.
set cmd [concat stap [stap_get_board_args] -d $testso -d $testexe -c "[stap_target_testdir]/$testexe_short" $testpath/$test.stp]
send_log "cmd: $cmd\n"
catch {eval exec $cmd} res
send_log "cmd output: $res\n"

set n 0
set m [llength [split $output "\n"]]
set expected [split $output "\n"]
foreach line [split $res "\n"] {
    if {![string equal $line [lindex $expected $n]]} {
      fail $test
      send_log "line [expr $n + 1]: expected \"[lindex $expected $n]\", "
      send_log "Got \"$line\"\n"
      return
    }
    incr n
}
if { $n != $m } {
  fail $test
  send_log "Got \"$n\" lines, expected \"$m\" lines\n"
} else {
  pass $test
}

if [is_remote target] {
  remote_exec target "rm [stap_target_testdir]/$testexe_short [stap_target_testdir]/$testso_short"
}

catch {exec rm -f $testexe $testso}
