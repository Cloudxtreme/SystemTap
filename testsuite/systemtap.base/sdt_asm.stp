probe process(@1).mark("a") 
{ if ($arg1 == 86) {printf ("PASS: %sh\n", $$name)}
  else {printf ("FAIL: %sh (%#x != 0x78)\n",$$name, $arg1)}
  if ($arg2 == 120) {printf ("PASS: %sl\n", $$name)}
  else {printf ("FAIL: %sl (%#x != 0x56)\n", $$name, $arg2)}
  if ($arg3 == 22136) {printf ("PASS: %sx\n", $$name)}
  else {printf ("FAIL: %sx (%#x != 0x5678)\n", $$name, $arg3)}}

